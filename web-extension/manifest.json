{
    "name": "Annotate",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "host_permissions": ["<all_urls>"]
}